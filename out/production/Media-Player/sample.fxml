<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<BorderPane prefHeight="340.0" prefWidth="553.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <center>
      <StackPane prefHeight="200.0" prefWidth="553.0" BorderPane.alignment="CENTER">
         <children>
            <MediaView fx:id="mediaView" fitHeight="200.0" fitWidth="200.0" StackPane.alignment="TOP_CENTER" />
            <VBox alignment="BOTTOM_CENTER" prefHeight="200.0" prefWidth="499.0" StackPane.alignment="BOTTOM_CENTER">
               <children>
                  <TextField fx:id="t1" prefHeight="31.0" prefWidth="192.0" />
                  <Slider fx:id="progressBar">
                     <VBox.margin>
                        <Insets bottom="-5.0" />
                     </VBox.margin>
                  </Slider>
                  <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#chooseFileMethod" text="choose files">
                           <HBox.margin>
                              <Insets left="7.0" right="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#play" text="play">
                           <HBox.margin>
                              <Insets left="7.0" right="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#stop" text="Stop">
                           <HBox.margin>
                              <Insets left="7.0" right="7.0" />
                           </HBox.margin></Button>
                        <Button mnemonicParsing="false" onAction="#pause" text="pause">
                           <HBox.margin>
                              <Insets left="7.0" right="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#fastForward" text="&gt;&gt;">
                           <HBox.margin>
                              <Insets left="7.0" right="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#slowRate" text="&lt;&lt;">
                           <HBox.margin>
                              <Insets left="7.0" right="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" text="+10s">
                           <HBox.margin>
                              <Insets left="7.0" right="7.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" text="-10s">
                           <HBox.margin>
                              <Insets left="7.0" right="7.0" />
                           </HBox.margin>
                        </Button>
                        <Slider fx:id="volumeSlider" orientation="VERTICAL">
                           <HBox.margin>
                              <Insets left="7.0" />
                           </HBox.margin>
                        </Slider>
                     </children>
                  </HBox>
               </children>
                  </VBox>
         </children>
      </StackPane>
   </center>
</BorderPane>
